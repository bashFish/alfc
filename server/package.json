{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && ncc build index.ts",
    "build:cpuoc": "csc /reference:native\\windows\\IntelOverclockingSDK.dll /t:library /out:native\\windows\\CPUOC.dll native\\windows\\CPUOC.cs",
    "build:wmiapi": "csc /t:library /out:native\\windows\\WmiAPI.dll native\\windows\\WmiAPI.cs",
    "lint": "eslint --ignore-path .gitignore --ext=.ts,.tsx .",
    "start": "nodemon",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "cli:win": "ts-node windows\\acpi.ts"
  },
  "dependencies": {
    "@types/ws": "7.2.6",
    "edge-js": "14.3.1",
    "express": "4.17.1",
    "is-elevated": "3.0.0",
    "ts-node": "8.10.2",
    "ws": "7.3.1"
  },
  "devDependencies": {
    "@types/express": "4.17.7",
    "@types/node": "14.0.27",
    "@typescript-eslint/eslint-plugin": "2.10.0",
    "@typescript-eslint/parser": "2.10.0",
    "@vercel/ncc": "0.23.0",
    "cross-env": "7.0.2",
    "eslint": "7.6.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-prettier": "3.1.1",
    "json-stringify-pretty-compact": "2.0.0",
    "kill-port": "1.6.1",
    "nodemon": "2.0.4",
    "prettier": "1.19.1",
    "rimraf": "3.0.2",
    "typescript": "3.9.7"
  }
}
